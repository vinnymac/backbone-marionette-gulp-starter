{
  "name": "Starter",
  "author": "Vincent Taverna",
  "version": "0.0.1",
  "description": "A frontend application using hamlc, coffeescript, and sass.",
  "repository": {
    "type": "git",
    "url": "git@github.com:vinnymac/backbone-marionette-gulp-starter.git"
  },
  "browser": {
    "plugin": "./vendor/scripts/jquery-plugin.js",
    "config": "./config/index.coffee"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "haml-coffee-browserify",
      "jstify",
      "envify"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~1.9.0",
    "browserify": "~8.1.0",
    "browserify-shim": "~3.8.2",
    "coffee-script": "~1.8.0",
    "coffeeify": "~1.0.0",
    "connect": "~3.3.4",
    "envify": "~3.2.0",
    "exorcist": "~0.1.6",
    "gulp": "~3.8.10",
    "gulp-changed": "~1.1.0",
    "gulp-clean": "~0.3.1",
    "gulp-compass": "~1.3.1",
    "gulp-concat": "~2.4.3",
    "gulp-file-include": "~0.8.0",
    "gulp-haml-coffee": "~0.0.6",
    "gulp-if": "~1.2.4",
    "gulp-imagemin": "~1.0.1",
    "gulp-jasmine": "~2.0.0",
    "gulp-livereload": "~3.4.0",
    "gulp-minify-css": "~0.3.12",
    "gulp-notify": "~2.1.0",
    "gulp-open": "~0.3.1",
    "gulp-plumber": "~0.6.6",
    "gulp-rev-all": "~0.7.5",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "~1.0.2",
    "gulp-util": "~3.0.2",
    "haml-coffee-browserify": "~0.0.4",
    "jstify": "~0.7.0",
    "mime-types": "~2.0.7",
    "pretty-hrtime": "~1.0.0",
    "underscore": "~1.7.0",
    "vinyl-source-stream": "~1.0.0",
    "vinyl-transform": "1.0.0",
    "watchify": "https://github.com/cconger/watchify/tarball/35407c030db9c6c9099e7fc2a5c21ac5eb7db5f8"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "backbone.marionette": "~2.3.0",
    "jquery": "~2.1.1"
  }
}
